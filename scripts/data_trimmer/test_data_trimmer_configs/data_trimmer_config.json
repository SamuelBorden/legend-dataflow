{
    "outputs" : [ "windowed", "presummed" ],
    "processors" : {
        "windowed": {
            "function": "double_windower",
            "module": "pygama.dsp.processors",
            "args": ["waveform", "1000", "1000", "windowed(len(waveform)-2000, 'f')"],
            "unit": "ADC"
        },
        
        "presummed": {
            "function": "presum",
            "module": "pygama.dsp.processors",
            "args": ["waveform", "presummed(len(waveform)/4, 'f')"],
            "unit": "ADC"
        }
    }
 }